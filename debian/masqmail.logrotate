/var/log/masqmail/masqmail.log {
	rotate 2
	weekly
	compress
	delaycompress
	missingok
	notifempty
	postrotate
		/etc/init.d/masqmail reload > /dev/null
	endscript
}
/var/log/masqmail/debug.log {
	rotate 2
	weekly
	compress
	delaycompress
	missingok
	notifempty
	postrotate
		/etc/init.d/masqmail reload > /dev/null
	endscript
}
